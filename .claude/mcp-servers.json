{
  "mcpServers": {
    "cdb-interactive": {
      "type": "stdio",
      "command": "cmd.exe",
      "args": [
        "/c", "python",
        "/c/WORK/mcp-cdb-interactive/server.py",
        "--timeout", "120",
        "--verbose"
      ],
      "env": {
        "_NT_SYMBOL_PATH": "SRV*C:\\Symbols*https://msdl.microsoft.com/download/symbols"
      }
    },
    "pal": {
      "type": "sse",
      "url": "http://127.0.0.1:4801/sse"
    },
    "atlassian": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.atlassian.com/v1/mcp"
      ],
      "env": {}
    },
    "google-workspace": {
      "type": "http",
      "url": "http://127.0.0.1:4802/mcp"
    }
  }
}
