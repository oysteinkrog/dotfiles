{
  "mcpServers": {
    "pal": {
      "type": "stdio",
      "command": "/c/WORK/pal-mcp-server/.pal_venv/bin/python",
      "args": [
        "/c/WORK/pal-mcp-server/server.py"
      ],
      "env": {}
    },
    "atlassian": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "mcp-remote",
        "https://mcp.atlassian.com/v1/mcp"
      ],
      "env": {}
    },
    "google-workspace": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@presto-ai/google-workspace-mcp"
      ],
      "env": {}
    }
  }
}
